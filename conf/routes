# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET           /                              controllers.HomeController.index

GET           /api/Company                   controllers.CompanyController.getAllCompany()
GET           /api/Company/:id               controllers.CompanyController.getCompany(id: Long)
POST          /api/Company                   controllers.CompanyController.postCompany()
PUT           /api/Company                   controllers.CompanyController.putCompany()
DELETE        /Api/Company/:id               controllers.CompanyController.deleteCompany(id:Long)

GET           /api/BillOfLoading             controllers.BillOfLoadingController.getAllBillOfLoading()
GET           /api/BillOfLoading/:id         controllers.BillOfLoadingController.getBillOfLoading(id: Long)
POST          /api/BillOfLoading             controllers.BillOfLoadingController.postBillOfLoading()
PUT           /api/BillOfLoading             controllers.BillOfLoadingController.putBillOfLoading()
DELETE        /Api/BillOfLoading/:id         controllers.BillOfLoadingController.deleteBillOfLoading(id:Long)

GET           /api/Container                 controllers.ContainerController.getAllContainer()
GET           /api/Container/:id             controllers.ContainerController.getContainer(id: Long)
POST          /api/Container                 controllers.ContainerController.postContainer()
PUT           /api/Container                 controllers.ContainerController.putContainer()
DELETE        /Api/Container/:id             controllers.ContainerController.deleteContainer(id:Long)

GET           /api/Fine                      controllers.FineController.getAllFine()
GET           /api/Fine/:id                  controllers.FineController.getFine(id: Long)
POST          /api/Fine                      controllers.FineController.postFine()
PUT           /api/Fine                      controllers.FineController.putFine()
DELETE        /Api/Fine/:id                  controllers.FineController.deleteFine(id:Long)

GET           /api/Order                     controllers.OrderController.getAllOrder()
GET           /api/Order/:id                 controllers.OrderController.getOrder(id: Long)
POST          /api/Order                     controllers.OrderController.postOrder()
PUT           /api/Order                     controllers.OrderController.putOrder()
DELETE        /Api/Order/:id                 controllers.OrderController.deleteOrder(id:Long)

GET           /api/Product                   controllers.ProductController.getAllProduct()
GET           /api/Product/:id               controllers.ProductController.getProduct(id: Long)
POST          /api/Product                   controllers.ProductController.postProduct()
PUT           /api/Product                   controllers.ProductController.putProduct()
DELETE        /Api/Product/:id               controllers.ProductController.deleteProduct(id:Long)

GET           /api/ProductType               controllers.ProductTypeController.getAllProductType()
GET           /api/ProductType/:id           controllers.ProductTypeController.getProductType(id: Long)
POST          /api/ProductType               controllers.ProductTypeController.postProductType()
PUT           /api/ProductType               controllers.ProductTypeController.putProductType()
DELETE        /Api/ProductType/:id           controllers.ProductTypeController.deleteProductType(id:Long)

GET           /api/Shipment                  controllers.ShipmentController.getAllShipment()
GET           /api/Shipment/:id              controllers.ShipmentController.getShipment(id: Long)
POST          /api/Shipment                  controllers.ShipmentController.postShipment()
PUT           /api/Shipment                  controllers.ShipmentController.putShipment()
DELETE        /Api/Shipment/:id              controllers.ShipmentController.deleteShipment(id:Long)

GET           /api/ShipmentReport            controllers.ShipmentReportController.getAllShipmentReport()
GET           /api/ShipmentReport/:id        controllers.ShipmentReportController.getShipmentReport(id: Long)
POST          /api/ShipmentReport            controllers.ShipmentReportController.postShipmentReport()
PUT           /api/ShipmentReport            controllers.ShipmentReportController.putShipmentReport()
DELETE        /Api/ShipmentReport/:id        controllers.ShipmentReportController.deleteShipmentReport(id:Long)

GET           /api/Truck                     controllers.TruckController.getAllTruck()
GET           /api/Truck/:id                 controllers.TruckController.getTruck(id: Long)
POST          /api/Truck                     controllers.TruckController.postTruck()
PUT           /api/Truck                     controllers.TruckController.putTruck()
DELETE        /Api/Truck/:id                 controllers.TruckController.deleteTruck(id:Long)

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file                  controllers.Assets.versioned(path="/public", file: Asset)
